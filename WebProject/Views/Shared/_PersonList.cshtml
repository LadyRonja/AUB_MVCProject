@using WebProject.Models;
@model List<Person>

@for (int i = 0; i < Model.Count; i++)
{
    <div class="container">
        <div class="row">
            <div class="col-sm">
                ID: @i
            </div>
            <div class="col-sm">
                @Model[i].Name
            </div>
        </div>
    </div>
}

